{
  "include": [
    "src/*"
  ],
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}